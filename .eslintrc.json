{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": "off",
        "import/extensions": ["error", "ignorePackages"],
        "import/prefer-default-export": "off"
    }
}
